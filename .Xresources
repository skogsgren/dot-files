xterm*VT100.geometry: 93x37+0+23
xterm*faceName: Input Mono Narrow
Xft.dpi: 120
xterm*faceSize: 10
xterm*renderFont: true
xterm*metaSendsEscape: true
xterm*forceBoxChars: true
xterm*.Translations: #override \
    Ctrl <Key> minus: smaller-vt-font() \n\
    Ctrl <Key> plus: larger-vt-font() \n\
    Shift <KeyPress> Insert: insert-selection(CLIPBOARD) \n\
    Ctrl Shift <Key>V:    insert-selection(CLIPBOARD) \n\
    Shift Alt <Key>V: insert-selection(PRIMARY) \n\
    Ctrl Shift <Key>C:    copy-selection(CLIPBOARD) \n\
    Ctrl <Btn1Up>: exec-formatted("xdg-open '%t'", PRIMARY)

! JCS colorscheme
! https://github.com/jcs/dotfiles/blob/master/.config/kitty/kitty.conf

! special
*.foreground:   #222222
*.background:   #f6f2de
*.cursorColor:  #222222

! black
*.color0:       #222222
*.color8:       #111111

! red
*.color1:       #dc322f
*.color9:       #cb4b16

! green
*.color2:       #859900
*.color10:      #93a1a1

! yellow
*.color3:       #b58900
*.color11:      #839496

! blue
*.color4:       #268bd2
*.color12:      #657b83

! magenta
*.color5:       #d33682
*.color13:      #6c71c4

! cyan
*.color6:       #2aa198
*.color14:      #586e75

! white
*.color7:       #d9d7cc
*.color15:      #e5e5e5
