xterm*VT100.geometry: 93x25+0+23
xterm*faceName: SF Mono
Xft.dpi: 120
xterm*faceSize: 10
xterm*renderFont: true
xterm*metaSendsEscape: true
xterm*forceBoxChars: true
xterm*.Translations: #override \
    Ctrl <Key> minus: smaller-vt-font() \n\
    Ctrl <Key> plus: larger-vt-font() \n\
    Shift <KeyPress> Insert: insert-selection(CLIPBOARD) \n\
    Ctrl Shift <Key>V:    insert-selection(CLIPBOARD) \n\
    Shift Alt <Key>V: insert-selection(PRIMARY) \n\
    Ctrl Shift <Key>C:    copy-selection(CLIPBOARD) \n\
    Ctrl <Btn1Up>: exec-formatted("xdg-open '%t'", PRIMARY)


! paramount.vim/default GVim colorscheme

*.foreground:   #262626
*.background:   #FFFFFF
*.cursorColor:  #262626

! black
*.color0:       #262626
*.color8:       #000000

! red
*.color1:       #FF0000
*.color9:       #FF0000

! green
*.color2:       #2e8b57
*.color10:      #2e8b57

! yellow
*.color3:       #fbbc04
*.color11:      #fbbc04

! blue
*.color4:       #1717ff
*.color12:      #1717ff

! magenta
*.color5:       #ff27ff
*.color13:      #ff27ff

! cyan
*.color6:       #00ffff
*.color14:      #00ffff

! white
*.color7:       #a8a8a8
*.color15:      #f1f1f1
